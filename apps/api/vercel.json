{
    "version": 2,
    "buildCommand": "cd ../.. && pnpm install && pnpm turbo run build --filter=@bookly/api...",
    "installCommand": "cd ../.. && pnpm install",
    "outputDirectory": "dist",
    "functions": {
        "dist/index.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ]
}