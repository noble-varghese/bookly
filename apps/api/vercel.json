{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/graphql(.*)",
            "dest": "dist/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}